   "source": [
    "def calc(n):\n",
    "    sumOfDigit = 0\n",
    "    powerOf2 = 2**n\n",
    "    string = str(powerOf2)\n",
    "    for digit in string:\n",
    "        sumOfDigit = int(digit) + sumOfDigit\n",
    "    return sumOfDigit"
   ]
  }
